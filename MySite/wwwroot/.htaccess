//Rewrite to www
Options +FollowSymLinks
RewriteEngine on
RewriteCond %{HTTP_HOST} ^milengeorgiev.co.uk[nc]
RewriteRule ^(.*)$ http://www.milengeorgiev.co.uk/$1 [r=301,nc]

//Caching schema
<FilesMatch "\.(jpg|png|css|js|svg|jpeg)$">
Header set Cache-Control "public, proxy-revalidate, max-age=160704000"

<ifModule mod_gzip.c>
mod_gzip_on Yes
mod_gzip_dechunk Yes
mod_gzip_item_include file .(html?|txt|css|js|php|pl|svg)$
mod_gzip_item_include handler ^cgi-script$
mod_gzip_item_include mime ^css/.*
mod_gzip_item_include mime ^js/.*
mod_gzip_item_include mime ^json/.*
mod_gzip_item_include mime ^svg/.*
mod_gzip_item_exclude mime ^image/.*
mod_gzip_item_exclude rspheader ^Content-Encoding:.*gzip.*
</ifModule>