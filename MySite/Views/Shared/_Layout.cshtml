<body onclick="removeHighlight()">
@{
    if (ViewBag.Title == ErrorController.NotFoundViewTitle || ViewBag.Title == ErrorController.ServerErrorViewTitle)
    {
        await Html.RenderPartialAsync("Headers/HeaderProblem");
    }
    else
    {
        await Html.RenderPartialAsync("Headers/HeaderOK");
    }
}

<div id="main-content" class="container col-11 rounded-img">
